  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.jsx",
      "!src/index.jsx",
      "!src/**/index.jsx",
      "!src/assets",
      "!src/registerServiceWorker.jsx",
      "!src/setupTests.jsx",
      "!src/tempPolyfills.jsx"
    ]
  },
