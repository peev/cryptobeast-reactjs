{
  "name": "material-dashboard-react",
  "description": "Material Dashboard ReactJS theme with Electron",
  "author": "Krasen Ilkov",
  "version": "1.0.0",
  "private": true,
  "homepage": "./",
  "main": "electron/electron-starter.js",
  "scripts": {
    "start": "nf start -p 3000",
    "server": "node ./api/server.js",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-app-rewired eject",
    "electron": "electron .",
    "electron-start": "node electron/electron-wait-react",
    "react-start": "react-app-rewired start",
    "pack": "build --dir",
    "dist": "npm run build && build",
    "rebuild": "electron-rebuild -f -w sqlite3"
  },
  "build": {
    "appId": "com.electron.electron-with-create-react-app",
    "win": {
      "iconUrl": "https://cdn2.iconfinder.com/data/icons/designer-skills/128/react-256.png"
    },
    "directories": {
      "buildResources": "assets"
    }
  },
  "dependencies": {
    "@material-ui/icons": "^1.0.0-beta.43",
    "axios": "^0.18.0",
    "babel-plugin-styled-components": "^1.5.1",
    "chartist": "^0.10.1",
    "classnames": "^2.2.5",
    "highcharts": "^6.0.0",
    "highcharts-more": "^0.1.7",
    "kraken-api": "^1.0.0",
    "material-ui": "1.0.0-beta.30",
    "material-ui-icons": "1.0.0-beta.17",
    "mobx": "^4.0.0",
    "mobx-react": "^5.0.0",
    "node-bittrex-api": "^0.8.3",
    "npm-run-all": "^4.1.1",
    "perfect-scrollbar": "^1.3.0",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.6.1",
    "react": "^16.3.0",
    "react-app-rewire-mobx": "^1.0.7",
    "react-app-rewired": "^1.5.0",
    "react-chartist": "^0.13.1",
    "react-dom": "^16.3.0",
    "react-google-maps": "^9.4.5",
    "react-highcharts": "^16.0.2",
    "react-jsx-highcharts": "^2.2.1",
    "react-jsx-highstock": "^2.2.1",
    "react-material-ui-form-validator": "^2.0.0-beta.9",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "react-select": "^1.2.1",
    "react-swipeable-views": "^0.12.12",
    "sequelize": "^4.37.6",
    "sqlite": "^2.9.1",
    "sqlite3": "^3.1.13"
  },
  "devDependencies": {
    "@types/cors": "^2.8.3",
    "@types/morgan": "^1.7.35",
    "cors": "^2.8.4",
    "devtron": "^1.4.0",
    "electron": "^1.8.2",
    "electron-builder": "^20.0.7",
    "electron-packager": "^11.0.1",
    "electron-rebuild": "^1.7.3",
    "eslint-config-airbnb": "^16.1.0",
    "foreman": "^2.0.0",
    "morgan": "^1.9.0",
    "node-gyp": "^3.6.2",
    "node-pre-gyp": "^0.6.39"
  }
}
