{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "lint": "./node_modules/eslint/bin/eslint.js . --fix",
    "start": "node ./node_modules/nodemon/bin/nodemon ./bin/www.js",
    "forever": "mkdir -p log && node ./node_modules/forever/bin/forever start -p ./log -a --silent --uid cryptobeast  ./bin/www.js",
    "clear:forever": "node ./node_modules/forever/bin/forever stop cryptobeast && rm -rf log/",
    "list:forever": "node ./node_modules/forever/bin/forever list"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "colors": "^1.2.4",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "cron": "^1.3.0",
    "debug": "~2.6.9",
    "dotenv": "^5.0.1",
    "express": "~4.16.0",
    "express-jwt": "^5.3.1",
    "forever": "^0.15.3",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "jwks-rsa": "^1.2.1",
    "kraken-api": "^1.0.0",
    "morgan": "^1.9.0",
    "node-bittrex-api": "^0.8.3",
    "nodemon": "^1.18.3",
    "pg": "^7.4.2",
    "pg-hstore": "^2.3.2",
    "request": "^2.87.0",
    "requestretry": "^2.0.0",
    "sequelize": "^4.37.6",
    "validator": "^10.4.0"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0"
  }
}
